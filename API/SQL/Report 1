SELECT P.UNIQUE_ID, P.STATUS, P.STATE, P.MUNICIPALITY, P.COUNTY, P.STANDARDIZED_LAND_USE_DESC_PROPERTY_TYPE, P.PARCEL_ID, P.CERTIFICATE, P.TOTAL_MARKET_VALUE, P.TOTAL_ASSESSED_VALUE,
(SELECT AMOUNT FROM FEES WHERE CATEGORY = 1 AND UNIQUE_ID = P.UNIQUE_ID) AS 'BEGINNING_BALANCE',
(SELECT EFFECTIVE_DATE FROM FEES WHERE CATEGORY = 1 AND UNIQUE_ID = P.UNIQUE_ID) AS 'BEGINNING_BALANCE_EFFECTIVE_DATE',
(SELECT SUM(AMOUNT) FROM FEES WHERE CATEGORY = 3 AND UNIQUE_ID = P.UNIQUE_ID) AS 'TOTAL_SUBS',
(SELECT SUM(AMOUNT) FROM FEES WHERE CATEGORY = 9 AND UNIQUE_ID = P.UNIQUE_ID) AS 'TOTAL_REFUNDS',
(SELECT SUM(AMOUNT) FROM FEES WHERE CATEGORY = 2 AND UNIQUE_ID = P.UNIQUE_ID) AS 'PREMIUMS'

FROM PARCELS P
